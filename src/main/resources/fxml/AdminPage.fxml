<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>

<VBox prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.AdminPageController">
   <children>
       <Pane prefHeight="115.0" styleClass="bh-white" VBox.vgrow="NEVER">
           <children>
               <ImageView fitHeight="136.0" fitWidth="432.0" layoutX="14.0" layoutY="17.0" pickOnBounds="true" preserveRatio="true">
                   <image>
                       <Image url="@../images/BWHlogo.gif" />
                   </image>
               </ImageView>
           </children>
       </Pane>
       <Pane prefHeight="12.0" styleClass="bh-accent" stylesheets="@../css/normal.css" VBox.vgrow="NEVER" />
      <BorderPane VBox.vgrow="ALWAYS">
         <center>
            <VBox maxHeight="500.0" maxWidth="500.0" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" prefHeight="60.0" prefWidth="527.0" text="Edit Directory" onAction="#editDirectory"/>
                  <Button mnemonicParsing="false" prefHeight="58.0" prefWidth="506.0" text="Edit Map" onAction="#editMap"/>
               </children>
            </VBox>
         </center>
      </BorderPane>
   </children>

</VBox>
