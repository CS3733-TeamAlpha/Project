<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.StartupController">
   <children>
      <Pane prefHeight="115.0" prefWidth="1280.0" styleClass="bh-white">
         <children>
            <ImageView fitHeight="136.0" fitWidth="432.0" layoutX="14.0" layoutY="17.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/BWHlogo.gif" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <Pane prefHeight="12.0" styleClass="bh-accent" stylesheets="@../css/normal.css" />
      <StackPane VBox.vgrow="ALWAYS">
         <children>
            <ImageView fitHeight="593.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true" styleClass="hc-invisible">
               <image>
                  <Image url="@../images/Network2.jpg" />
               </image>
               <viewport>
                  <Rectangle2D height="593.0" width="1280.0" />
               </viewport>
            </ImageView>
            <BorderPane prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
               <bottom>
                  <AnchorPane prefHeight="40.0" BorderPane.alignment="BOTTOM_RIGHT">
                     <children>
                        <ImageView fx:id="eyeImage" fitHeight="31.0" fitWidth="34.0" layoutX="1157.0" layoutY="5.0" onMouseClicked="#toggleHighContrast" pickOnBounds="true" preserveRatio="true" y="4.0" AnchorPane.rightAnchor="92.0">
                           <image>
                              <Image url="@../images/ic_eye.png" />
                           </image>
                        </ImageView>
                        <ImageView fx:id="lockImage" fitHeight="37.0" fitWidth="31.0" layoutX="1199.0" layoutY="5.0" onMouseClicked="#showLogin" pickOnBounds="true" preserveRatio="true" AnchorPane.rightAnchor="50.0">
                           <image>
                              <Image url="@../images/ic_lock.png" />
                           </image>
                        </ImageView>
                     </children>
                  </AnchorPane>
               </bottom>
               <center>
                  <StackPane BorderPane.alignment="CENTER">
                     <children>
                        <VBox alignment="CENTER" fillWidth="false" spacing="15.0" BorderPane.alignment="CENTER">
                           <children>
                              <Label text="Find Directions" textFill="WHITE">
                                 <font>
                                    <Font name="Glacial Indifference Regular" size="96.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="searchBox" prefHeight="75.0" prefWidth="700.0" promptText="Enter Location or Provider Search...">
                                 <font>
                                    <Font size="33.0" />
                                 </font>
                              </TextField>
                              <Button mnemonicParsing="false" onAction="#showMap" styleClass="bh-button" stylesheets="@../css/normal.css" text="View Map">
                                 <font>
                                    <Font name="Glacial Indifference Regular" size="33.0" />
                                 </font>
                              </Button>
                           </children>
                           <opaqueInsets>
                              <Insets />
                           </opaqueInsets>
                        </VBox>
                     </children>
                  </StackPane>
               </center>
            </BorderPane>
         </children>
      </StackPane>
   </children>
</VBox>
